<?xml version="1.0" encoding="UTF-8"?>
 	<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
 	    "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">

<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>1.2</jsp-version>
	<short-name>exam-authority</short-name>
	<uri>http://www.exam.com/tags/authority</uri>

	<tag>
		<name>authority</name>
		<tag-class>cn.newtouch.web.taglib.AuthorityTag</tag-class>
		<attribute>
			<name>subjectPoolId</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>username</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>authfunc</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
</taglib>